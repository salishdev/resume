\documentclass{resume} % Use the custom resume.cls style

\usepackage[left=0.4 in,top=0.4in,right=0.4 in,bottom=0.4in]{geometry} % Document margins
\newcommand{\tab}[1]{\hspace{.2667\textwidth}\rlap{#1}}
\newcommand{\itab}[1]{\hspace{0em}\rlap{#1}}

%----------------------------------------------------------------------------------------
% Contact Information
%----------------------------------------------------------------------------------------

\name{<% name %>}
\address{<% contact.location %> \\
    \href{tel:<% contact.phone_clean %>}{<% contact.phone %>} \\
    \href{mailto:<% contact.email %>}{<% contact.email %>} \\
    \href{https://<% contact.website %>}{<% contact.website %>} \\
    \href{https://<% contact.github %>}{<% contact.github %>} \\
    \href{https://<% contact.linkedin %>}{<% contact.linkedin %>}
}

\begin{document}

%----------------------------------------------------------------------------------------
% Professional Summary
%----------------------------------------------------------------------------------------

<%#summary%>

% \begin{rSection}{Professional Summary}
  <%.%>
% \end{rSection}

<%/summary%>

%----------------------------------------------------------------------------------------
% Work Experience
%----------------------------------------------------------------------------------------
<%#experience.length%>
  \begin{rSection}{Work Experience}

    <%#experience%>
        \begin{rWorkHistoryItem}{<% position %>}{<% company %>}{<% duration %>}
      <%#accomplishments%>
            \item <%.%>
      <%/accomplishments %>
        \end{rWorkHistoryItem}
    <%/experience%>

  \end{rSection}
<%/experience.length%>

%----------------------------------------------------------------------------------------
% Skills
%----------------------------------------------------------------------------------------

<%#skills.length%>
  \begin{rSection}{Skills}
      \begin{tabular}{ @{} >{\bfseries}l @{\hspace{6ex}} l }
        <%#skills%>
          <%category%> & <% items %> \\
        <%/skills%>
      \end{tabular}
  \end{rSection}
<%/skills.length%>


%----------------------------------------------------------------------------------------
% Education
%----------------------------------------------------------------------------------------

<%#education.length%>
  \begin{rSection}{Education}
    <%#education%>
      \textbf{<% degree %>}, <% institution %> \hfill {<% duration %>}
    <%/education%>
  \end{rSection}
<%/education.length%>

%----------------------------------------------------------------------------------------
% Projects
%----------------------------------------------------------------------------------------

<%#projects.length%>
 \begin{rSection}{Projects}
  <%#projects%>

     \textbf{<% role %>}, {<% name %>} <%#link%>(\href{<% url %>}{<% title %>})<%/link%> \hfill {MMM YYYY --- MMM YYYY}
     \begin{itemize}
         \item \href{https://www.faangtechleads.com/resume/general-guidelines}{Action verb} + what you did + reason, outcome or quantified results. Talk about technologies used
         \item \href{https://www.faangtechleads.com/resume/general-guidelines}{Action verb} + what you did + reason, outcome or quantified results. Talk about technologies used
     \end{itemize}
  <%/projects%>
 \end{rSection}
<%/projects.length%>

%----------------------------------------------------------------------------------------
% Awards
%----------------------------------------------------------------------------------------

% \begin{rSection}{Awards}

%     \begin{itemize}
%         \item \textbf{ABC Hackathon} --- 1st place \hfill {MMM YYYY --- MMM YYYY}
%         \item \textbf{XYZ Competition} --- 2nd place \hfill {MMM YYYY --- MMM YYYY}
%     \end{itemize}

% \end{rSection}

\end{document}
